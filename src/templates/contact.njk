<form
  accept-charset="UTF-8"
  action="https://somlor.wufoo.com/forms/z1sm0ne11t6m2fw/#public"
  class="contact"
  enctype="multipart/form-data"
  id="contact-form"
  method="post"
  name="contact-form"
  novalidate>
  <div class="contact--info">
    <input
      id="Field3"
      placeholder="Your Name"
      name="Field3"
      type="text"
      value=""
      maxlength="255"
      tabindex="1"
      onkeyup=""
      required>
    <input
      id="Field4"
      placeholder="Your Email"
      name="Field4"
      type="email"
      spellcheck="false"
      value=""
      maxlength="255"
      tabindex="2"
      required>
    <input
      id="Field6"
      placeholder="Your Phone Number (optional)"
      name="Field6"
      tabindex="3"
      type="tel"
      maxlength="255"
      value="">
  </div>
  <div class="contact--message">
    <textarea
      id="Field5"
      placeholder="Your Message"
      name="Field5"
      spellcheck="true"
      rows="2"
      cols="40"
      tabindex="4"
      onkeyup=""
      required></textarea>
  </div>
  <div class="contact--send">
    <input
      type="hidden"
      id="idstamp"
      name="idstamp"
      value="Y5R7YZ3hKc1o7J7kB0Gngu6Ss3rBdYooAe1JO0U0/1Y=">
    <input
      id="saveForm"
      name="saveForm"
      class="btTxt submit"
      type="submit"
      value="Send Email">
  </div>
</form>
